{
    "version": 2,
    "name": "portfolio-gina",
    "builds": [
       { "src": "./index.js", "use": "@vercel/node" }
    ],
    "routes": [
       { "src": "/views/home.ejs", "dest": "/" },
       { "src": "/views/about.ejs", "dest": "/overmij" },
       { "src": "/views/portfolio.ejs", "dest": "/portfolio" },
       { "src": "/views/prices.ejs", "dest": "/prices" },
       { "src": "/views/prices-package.ejs", "dest": "/prices/1-hour" },
       { "src": "/views/prices-package.ejs", "dest": "/prices/2-hours" },
       { "src": "/views/prices-package.ejs", "dest": "/prices/3-hours" },

       { "src": "/views/home.ejs", "dest": "/en" },
       { "src": "/views/en/about.ejs", "dest": "/en/overmij" },
       { "src": "/views/en/portfolio.ejs", "dest": "/en/portfolio" },
       { "src": "/views/en/prices.ejs", "dest": "/en/prices" },
       { "src": "/views/en/prices-package.ejs", "dest": "/en/prices/1-hour" },
       { "src": "/views/en/prices-package.ejs", "dest": "/en/prices/2-hours" },
       { "src": "/views/en/prices-package.ejs", "dest": "/en/prices/3-hours" }
    ]
 }